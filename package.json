{
  "name": "ember-assembly",
  "version": "1.2.1",
  "description": "A collection of beautiful UI components built by Goods",
  "keywords": [
    "ember-addon",
    "goods",
    "components",
    "ui"
  ],
  "repository": "https://github.com/goods/ember-assembly.git",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "postpublish": "ember ts:clean",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "ember-animated": "^1.0.4",
    "ember-auto-import": "^2.4.2",
    "ember-basic-dropdown": "^6.0.1",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^6.1.0",
    "ember-cli-moment-shim": "^3.8.0",
    "ember-cli-typescript": "^5.2.1",
    "ember-concurrency": "^1.3.0",
    "ember-concurrency-decorators": "2.0.1",
    "ember-css-modules": "^1.2.1",
    "ember-decorators": "^6.1.1",
    "ember-moment": "^10.0.0",
    "ember-svg-jar": "^2.2.3",
    "postcss-functions": "^3.0.0"
  },
  "devDependencies": {
